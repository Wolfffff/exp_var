@ARTICLE{Kuan2019-ju,
  title    = "Cell type-specific gene expression patterns associated with
              posttraumatic stress disorder in World Trade Center responders",
  author   = "Kuan, Pei-Fen and Yang, Xiaohua and Clouston, Sean and Ren, Xu
